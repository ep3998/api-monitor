    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/router": "^3.3.1",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-alpha.21",
    "body-parser": "~1.13.2",
    "bootstrap": "^4.0.0-alpha.6",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "debug": "~2.2.0",
    "express": "^4.15.2",
    "jade": "~1.11.0",
    "mongoose": "^4.9.1",
    "morgan": "~1.6.1",
    "rxjs": "^5.0.1",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.2"
