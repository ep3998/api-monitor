{
  "name": "api-monitor",
  "version": "1.0.0",
  "description": "API Monitor - MongoDB Server",
  "main": "index.js",
  "scripts": {
    "start-mine": "rimraf dist && webpack-dev-server --env=dev --inline --progress --port 8085",
    "backend": "nodemon --watch server ./server/bin/www",
    "backend-compile": "tsc server/*.ts --watch",
    "express": "node ./server/bin/www",
    "watch": "webpack --watch",
    "watch:dev": "npm run server & npm run watch",
    "clean:dist": "rimraf dist",
    "prebuild": "npm run clean:dist",
    "build:dev": "rimraf dist && webpack --env=dev --progress --profile --bail",
    "build:prod": "webpack --env=prod --progress",
    "prestart-old": "npm run build:dev",
    "server": "nodemon dist/server/index.js",
    "start": "npm run start-mine",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "typings": "typings"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ep3998/api-monitor.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ep3998/api-monitor/issues"
  },
  "homepage": "https://github.com/ep3998/api-monitor#readme",
